/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=selbar);
    Nand(a=b,b=sel,out=w1);
    Nand(a=selbar,b=a,out=w2);
    Nand(a=w1,b=w2,out=out);
}
